generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model CobbDaily {
  id                        String   @id @default(auto()) @map("_id") @db.ObjectId
  date                      DateTime
  day                       Int
  week                      Int
  quarantine_male           Int
  quarantine_female         Int
  mortality_male            Int
  mortality_female          Int
  recovery_male             Int
  recovery_female           Int
  total_number_male         Int
  total_number_female       Int
  Cummulative_mortality_fem Int
  male_feed_gms_per_bird    Float
  male_feed_total_kgm       Float
  female_feed_gms_per_bird  Float
  female_feed_total_kgm     Float
  cleanup_time              String
  h2o_consumption           Float
  floor_eggs                Int
  settable_eggs             Int
  broken                    Int
  damage                    Int
  softshells                Int
  large                     Int
  small                     Int
  abnormal                  Int
  total_eggs                Int
  daily_production          Float
  remarks                   String
}
